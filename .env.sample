# OpenCTI Platform Configuration
OPENCTI_URL=http://localhost:8080
OPENCTI_TOKEN=ChangeMe

# Connector Configuration
CONNECTOR_ID=ChangeMe
CONNECTOR_NAME=PortSpoofPro
CONNECTOR_SCOPE=portspoof-pro
CONNECTOR_LOG_LEVEL=info

# Queue Protocol: How bundles are sent to OpenCTI
# - amqp: RabbitMQ (only for self-hosted OpenCTI with RabbitMQ access)
CONNECTOR_QUEUE_PROTOCOL=api

# PortSpoofPro RabbitMQ Configuration
# For same-host deployment: amqp://portspoof:password@rabbitmq:5672/
# For remote deployment: amqp://portspoof:password@your-portspoof-host:5672/
RABBITMQ_URL=amqp://portspoof:changeme@localhost:5672/
RABBITMQ_QUEUE_NAME=opencti-connector-queue

# Optional: Logging Configuration
PS_LOG_FILE=
PS_DEBUG_FULL_DUMPS=0
